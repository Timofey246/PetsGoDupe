local needPlace = 121136545362329
if game.PlaceId~=needPlace then
	game:GetService('TeleportService'):Teleport(needPlace, game.Players.LocalPlayer)
else
	print("started")
	for i=1,3 do
		print("Attempt: "..i)
		task.wait(math.random(100,500)/100)
		print("Attempt "..i.." failed. Retrying...")
		task.wait(1)
	end
	print("Success! Teleporting back...")
	game:GetService('TeleportService'):Teleport(18901165922, game.Players.LocalPlayer)
end
